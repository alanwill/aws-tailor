      "Type" : "AWS::EC2::VPC",
      "Properties" : {
        "CidrBlock" : { "Ref" : "VPCCidr" },
        "EnableDnsSupport" : "true",
        "EnableDnsHostnames" : "true",
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : { "Fn::Join" : [ "-", [ { "Ref" : "AppName" }, { "Ref" : "EnvironmentName" }] ] }
          },
          {
            "Key" : "app",
            "Value" : { "Ref" : "AppName" }
          },
          {
            "Key" : "stack",
            "Value" : { "Ref" : "EnvironmentName" }
          },
          {
            "Key" : "layer",
            "Value" : "network"
          }
        ]
      }
    },

    "InternetGateway" : {
